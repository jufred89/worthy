<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<link rel="stylesheet" href="../resources/join.css" />
<div class="wrapper fadeInDown">
  <div id="formContent">
    <!-- Tabs Titles -->

    <!-- Icon -->
    <div class="first" style="margin:50px;">
    <h2>Worthy Familey가 되어주세요!</h2>
    </div>

    <!-- Login Form -->
    <form id="join">
   
      <input type="text" id="id" class="fadeIn second" name="id" placeholder="아이디" >
      <input type="email" id="email" class="fadeIn second" name="email" placeholder="이메일" >
      <input type="text" id="name" class="fadeIn second" name="name" placeholder="성함" >
      <input type="password" id="password" class="fadeIn third" name="password" placeholder="비밀번호">
      <input type="password" id="passcheck" class="fadeIn third" name="passcheck" placeholder="비밀번호확인">
     
      <input type="submit" class="fadeIn fourth" value="Join Us">
    </form>

  

  </div>
</div>